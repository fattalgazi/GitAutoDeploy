{
	"pidfilepath": "/var/run/gitautodeploy.pid",
	"host": "[LOCAL_IP]",
	"port": [LOCAL_PORT_TO_LISTEN],
	"global_deploy": [
		"echo Starting deploy!",
		"echo Ending deploy!"
	],
	"repositories":
	[
		{
			"url": "[GIT_REPO_URL]",
			"branch": "[BRANCH_NAME_TO_PULL]",
			"remote": "[REMOTE_NAME]",
			"path": "[PROJECT_FOLDER]",
			"deploy": "[AFTER_COMMANDS]"
	    }
    ]
}
